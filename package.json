{
  "name": "app-grader",
  "version": "0.0.1",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "start": "node build",
    "init-db": "sqlite3 app-grader.db '.read scripts/init-db.sql' && NODE_ENV=development node scripts/apps-loader.js 1",
    "test": "standard && node --test --loader=esmock",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync",
    "format": "standard --fix"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^1.0.0-next.73",
    "@sveltejs/kit": "1.0.0-next.350",
    "esmock": "^1.7.5",
    "standard": "^17.0.0",
    "svelte": "^3.44.0"
  },
  "type": "module",
  "dependencies": {
    "better-sqlite3": "^7.5.3",
    "dotenv": "^16.0.0",
    "google-auth-library": "^7.14.1",
    "googleapis": "^99.0.0"
  }
}

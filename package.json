{
  "name": "app-grader",
  "version": "0.0.1",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "NODE_ENV=development nodemon -e js,njk,css src/server.js",
    "start": "node src/server.js",
    "init-db": "sqlite3 app-grader.db '.read scripts/init-db.sql' && NODE_ENV=development node scripts/apps-loader.js 1",
    "test": "standard && node --test --loader=esmock",
    "format": "standard --fix"
  },
  "devDependencies": {
    "esmock": "^1.7.5",
    "nodemon": "^2.0.20",
    "standard": "^17.0.0"
  },
  "type": "module",
  "dependencies": {
    "better-sqlite3": "^8.0.0",
    "body-parser": "^1.20.1",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "google-auth-library": "^7.14.1",
    "googleapis": "^99.0.0",
    "htmx.org": "^1.8.5",
    "nunjucks": "^3.2.3"
  }
}
